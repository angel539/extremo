    <eStructuralFeatures xsi:type="ecore:EReference" name="interpreters" upperBound="-1"
        eType="#//constraints/ConstraintInterpreter" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="services" upperBound="-1"
        eType="#//queries/Service" containment="true"/>
  </eClassifiers>
  <eSubpackages name="constraints" nsURI="http://www.core.extremo/SemanticManager#constraints"
