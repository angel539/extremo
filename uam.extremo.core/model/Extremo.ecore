      <eStructuralFeatures xsi:type="ecore:EAttribute" name="body" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="appliedTo" eType="#//dataModel/NamedElement"
          eOpposite="#//dataModel/NamedElement/constraints"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="evaluations" upperBound="-1"
          eType="#//constraints/ConstraintResult" eOpposite="#//constraints/ConstraintResult/constraint"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConstraintInterpreter" abstract="true">
      <eOperations name="eval" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
        <eParameters name="constraint" eType="#//constraints/Constraint"/>
        <eParameters name="namedElement" eType="#//dataModel/NamedElement"/>
      </eOperations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="constraints" upperBound="-1"
          eType="#//constraints/Constraint"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="evals" upperBound="-1"
          eType="#//constraints/ConstraintResult"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConstraintResult">
      <eStructuralFeatures xsi:type="ecore:EReference" name="constraint" eType="#//constraints/Constraint"
          eOpposite="#//constraints/Constraint/evaluations"/>
