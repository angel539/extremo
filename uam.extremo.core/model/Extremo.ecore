      <eOperations name="search">
        <eParameters name="search" eType="#//queries/SearchResult"/>
      </eOperations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PredicateBasedSearch" eSuperTypes="#//queries/SimpleSearchConfiguration">
