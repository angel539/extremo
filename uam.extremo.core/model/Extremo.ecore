      </eOperations>
      <eOperations name="addSemanticNodeToSemanticGroup">
        <eParameters name="semanticGroup" eType="#//queries/SemanticGroup"/>
        <eParameters name="semanticNode" eType="#//dataModel/SemanticNode"/>
      </eOperations>
      <eOperations name="addDataPropertyToSemanticGroup">
        <eParameters name="semanticGroup" eType="#//queries/SemanticGroup"/>
        <eParameters name="dataProperty" eType="#//dataModel/DataProperty"/>
      </eOperations>
      <eOperations name="addObjectPropertyToSemanticGroup">
        <eParameters name="semanticGroup" eType="#//queries/SemanticGroup"/>
        <eParameters name="objectProperty" eType="#//dataModel/ObjectProperty"/>
      </eOperations>
