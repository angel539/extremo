          eType="#//queries/SearchOption" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="filterBy" eType="#//queries/DataModelType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeSearchConfiguration" eSuperTypes="#//queries/SearchConfiguration">
