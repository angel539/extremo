      <eStructuralFeatures xsi:type="ecore:EReference" name="unsat" upperBound="-1"
          eType="#//dataModel/NamedElement"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="queries" nsURI="http://www.core.extremo/SemanticManager#queries"
