        <eParameters name="key" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      </eOperations>
      <eOperations name="addNamedElementToSemanticGroup">
        <eParameters name="semanticGroup" eType="#//queries/SemanticGroup"/>
        <eParameters name="namedElement" eType="#//dataModel/NamedElement"/>
      </eOperations>
      <eOperations name="addResourceToSemanticGroup">
        <eParameters name="semanticGroup" eType="#//queries/SemanticGroup"/>
        <eParameters name="resource" eType="#//dataModel/Resource"/>
      </eOperations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="groups" upperBound="-1"
          eType="#//queries/SemanticGroup" containment="true"/>
    </eClassifiers>
