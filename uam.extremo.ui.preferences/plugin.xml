<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="uam.extremo.ui.preferences.page.RuleWeightPreferencePage"
            id="uam.extremo.ui.preferences.page"
            name="Extremo Prefs">
      </page>
   </extension>
   <extension
         point="extremo.core.preferences.weights">
      <preferencessettings
            class="uam.extremo.ui.preferences.page.AllTheSamePreferencePointSettings"
            id="uam.extremo.ui.preferences.settings.allthesame"
            name="All the Same">
      </preferencessettings>
      <preferencessettings
            class="uam.extremo.ui.preferences.page.DefinitionsPreferencePointSettings"
            id="uam.extremo.ui.preferences.settings.definitions"
            name="Give preference to the defitions">
      </preferencessettings>
      <preferencessettings
            class="uam.extremo.ui.preferences.page.SynonymsSharingPreferencePointSettings"
            id="uam.extremo.ui.preferences.settings.synonymssharing"
            name="Synonyms sharing between senses">
      </preferencessettings>
      <preferencessettings
            class="uam.extremo.ui.preferences.page.UsagesPreferencePointSettings"
            id="uam.extremo.ui.preferences.settings.usages"
            name="Give preference to the usages">
      </preferencessettings>
   </extension>
</plugin>
