<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="Extremo Drop Integration Test"
            icon="icons/thin_test_tube.png"
            category="metaRDF.ui"
            class="metardf.ui.temp.views.test.views.DropIntegrationTestView"
            id="metardf.ui.temp.views.test.views.DropIntegrationTestView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="metardf.ui.temp.views.test.views.TempView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="metaRDF.ui.drop">
      <viewdrop
            class="metardf.ui.temp.views.test.views.DropIntegrationTreeViewerDropAdapter"
            id="metaRDF.ui.temp.views.test.treedrop"
            name="metaRDF.ui.temp.views.test.treedrop">
      </viewdrop>
   </extension>

</plugin>
