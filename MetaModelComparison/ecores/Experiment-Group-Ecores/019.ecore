<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="main" nsURI="http://www.core.extremo/019/" nsPrefix="main">
  <eClassifiers xsi:type="ecore:EClass" name="Swap" eSuperTypes="#//DerivativeInstrument">
    <eStructuralFeatures xsi:type="ecore:EReference" name="hasLeg" lowerBound="2"
        upperBound="-1" eType="#//SwapLeg" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SwapLeg" eSuperTypes="#//CashflowTerms #//SwapTerms"/>
  <eClassifiers xsi:type="ecore:EClass" name="CashflowTerms"/>
  <eClassifiers xsi:type="ecore:EClass" name="SwapTerms"/>
  <eClassifiers xsi:type="ecore:EClass" name="DerivativeInstrument" eSuperTypes="#//NamedElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="SimpleTask" eSuperTypes="#//Task"/>
  <eClassifiers xsi:type="ecore:EClass" name="CoumpoundTask" eSuperTypes="#//Task">
    <eStructuralFeatures xsi:type="ecore:EReference" name="containsTask" upperBound="-1"
        eType="#//Task" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Task" abstract="true" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="refersTo" upperBound="-1"
        eType="#//DerivativeInstrument"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sender" upperBound="-1"
        eType="#//EthereumAccount/EthereumAccount"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Flow" eSuperTypes="#//ModelElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="BusinessProcess" eSuperTypes="#//ApplicationTask"/>
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ModelElement" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="ApplicationTask"/>
  <eClassifiers xsi:type="ecore:EClass" name="Company" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="accounts" upperBound="-1"
        eType="#//Account"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Account">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="balance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="periodStart" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SwapProcess">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="#//EthereumAccount/EthereumAccount"/>
  </eClassifiers>
  <eSubpackages name="Account" nsURI="http://www.core.extremo/019/Account" nsPrefix="Account">
    <eClassifiers xsi:type="ecore:EClass" name="DepositAccount" eSuperTypes="#//Transaction/Account"/>
    <eClassifiers xsi:type="ecore:EClass" name="LoanAccount" eSuperTypes="#//Transaction/Account">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="rate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Checking" eSuperTypes="#//Account.1/DepositAccount"/>
    <eClassifiers xsi:type="ecore:EClass" name="Savings" eSuperTypes="#//Account.1/DepositAccount"/>
    <eClassifiers xsi:type="ecore:EClass" name="CreditCard" eSuperTypes="#//Account.1/LoanAccount">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="creditLimit" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="gracePeriod" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Mortgage" eSuperTypes="#//Account.1/LoanAccount"/>
  </eSubpackages>
  <eSubpackages name="Transaction" nsURI="http://www.core.extremo/019/Transaction"
      nsPrefix="Transaction">
    <eClassifiers xsi:type="ecore:EClass" name="InternalTransaction" eSuperTypes="#//Transaction/Transaction">
      <eStructuralFeatures xsi:type="ecore:EReference" name="otherAccount" eType="#//Transaction/Account"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ExternalTransaction" eSuperTypes="#//Transaction/Transaction">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="processor" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="externalAccount" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="externalId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Account">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="balance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="periodStart" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Transaction">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="amount" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="date" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="comment" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="tradeLifecycleStage"
          lowerBound="1" eType="#//Transaction/TradeLifecycleStage"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="TradeLifecycleStage">
      <eLiterals name="OpenTrade" literal="OpenTrade"/>
      <eLiterals name="TerminatedTrade" value="1" literal="TerminatedTrade"/>
      <eLiterals name="ClearedTrade" value="2" literal="ClearedTrade"/>
      <eLiterals name="MaturedTrade" value="3" literal="MaturedTrade"/>
      <eLiterals name="ClosedTrade" value="4" literal="ClosedTrade"/>
      <eLiterals name="AmendedTrade" value="5" literal="AmendedTrade"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="EthereumAccount" nsURI="http://www.core.extremo/019/EthereumAccount"
      nsPrefix="EthereumAccount">
    <eClassifiers xsi:type="ecore:EClass" name="NormalAccount" eSuperTypes="#//EthereumAccount/EthereumAccount"/>
    <eClassifiers xsi:type="ecore:EClass" name="ContractEthereumAccount" eSuperTypes="#//EthereumAccount/EthereumAccount">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="storageRootContractEthereumAccount"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="codeHashContractEthereumAccount"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="EthereumAccount">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="nonceEthereumAccount"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="addressEthereumAccount"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="balanceEthereumAccount"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
