		<sm:fileAbbreviation>fibo-fbc-fct-rga</sm:fileAbbreviation>
		<sm:filename>RegulatoryAgencies.rdf</sm:filename>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/ontology/AboutTheEDMC-FIBOFamily/"/>
		<rdfs:seeAlso rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FBC/AboutFBC/"/>
